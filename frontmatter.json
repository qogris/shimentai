{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "posts",
      "pageBundle": true,
      "previewPath": "/posts",
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "authors",
          "name": "authors",
          "type": "choice",
          "choices": [
            "haifeng",
            "qogris"
          ]
        },
        {
          "title": "isCJKLanguage",
          "name": "isCJKLanguage",
          "type": "string"
        }
      ]
    },
    {
      "name": "projects",
      "pageBundle": true,
      "previewPath": "/projects",
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "authors",
          "name": "authors",
          "type": "choice",
          "choices": [
            "haifeng",
            "qogris"
          ]
        },
        {
          "title": "isCJKLanguage",
          "name": "isCJKLanguage",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts",
      "previewPath": "/posts",
      "trailingSlash": false
    },
    {
      "title": "projects",
      "path": "[[workspace]]/content/projects",
      "previewPath": "/projects",
      "trailingSlash": false
    }
  ],
  "frontMatter.git.enabled": false,
  "frontMatter.dashboard.openOnStart": true
}