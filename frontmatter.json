{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "posts",
      "pageBundle": true,
      "previewPath": "[[workspace]]/content/posts/",
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "authors",
          "name": "authors",
          "type": "choice",
          "choices": [
            "haifeng",
            "qogris"
          ]
        }
      ]
    },
    {
      "name": "projects",
      "pageBundle": true,
      "previewPath": "[[workspace]]/content/projects/",
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "authors",
          "name": "authors",
          "type": "choice",
          "choices": [
            "haifeng",
            "qogris"
          ]
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts"
    },
    {
      "title": "projects",
      "path": "[[workspace]]/content/projects"
    }
  ],
  "frontMatter.git.enabled": true
}